# Uncomment the next line to define a global platform for your project
target 'HeyOffice' do
  platform :ios, '10.0'
  use_frameworks!
  inhibit_all_warnings!

  # Pods for HeyOffice
  pod 'AWSCognito'
  pod 'AWSCognitoIdentityProvider'
  pod 'AWSDynamoDB'
  pod 'AWSLex'
  pod 'AWSS3'
  pod 'PKHUD'
  pod 'OAuthSwift'

  target 'HeyOfficeTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Quick'
    pod 'Nimble'
    pod 'SwiftLint'
  end

end
